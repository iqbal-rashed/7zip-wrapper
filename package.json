{
  "name": "7zip-wrapper",
  "version": "0.0.2",
  "description": "A comprehensive, feature-complete Node.js wrapper for 7-Zip with TypeScript support, bundled binaries, and cross-platform compatibility.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "7zip": "./dist/cli.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "postdownload": "node ./dist/postinstall.js",
    "postinstall": "node -e \"process.env.INIT_CWD !== process.cwd() && require('child_process').execSync('npm run postdownload', { stdio: 'inherit' });\""
  },
  "keywords": [
    "7zip",
    "7-zip",
    "archive",
    "compression",
    "extract",
    "zip",
    "7z",
    "tar",
    "gzip",
    "bzip2",
    "xz",
    "rar",
    "iso",
    "wrapper",
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Iqbal Rashed",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqbal-rashed/7zip-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/iqbal-rashed/7zip-wrapper/issues"
  },
  "homepage": "https://github.com/iqbal-rashed/7zip-wrapper#readme",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {}
}
